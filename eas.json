{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_URL_SERVEUR_API_DEV": "https://tracker-mtb-server.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_URL_SERVEUR_API_DEV": "https://tracker-mtb-server.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
